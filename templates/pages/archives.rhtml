<section class="archives">
	<header class="archives">
	<h1>the archives</h1>
	</header>
	<section class="archive-entries">
	  <% if archives.length > 0 %>
	    <% for entry in archives %>
	        <section class="archive-entry title"><h1><a href="<%= entry.path %>" title="<%= entry.title %>"><%= entry.title %></a></h1></section>
					<section class="archive-entry date"><%= entry.date %></section>
					<hr class="thin"/>
	    <% end %>
	  <% end %>
	</section>
</section>
